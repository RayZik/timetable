    "@types/express": "^4.0.32",
    "@types/express-session": "^0.0.32",
    "@types/jsonwebtoken": "^7.1.31",
    "@types/passport": "^0.2.34",
    "@types/passport-local": "^1.0.29",
    "angular2-moment": "^1.1.0",
    "@angular/forms": "~2.1.1",
    "atoa": "^1.0.0",
